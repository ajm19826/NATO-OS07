        --symbol-size: var(--sl-font-size-2x-small);
        --symbol-color-active: var(--sl-color-neutral-500);
        --symbol-spacing: 0;
    }

    .product .rating-element, .product .rating-count {
        color: var(--sl-color-neutral-600);
