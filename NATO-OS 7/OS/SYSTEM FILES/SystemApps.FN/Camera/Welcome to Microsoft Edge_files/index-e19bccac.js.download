    /** common story styles */
    .story-dialog {
        --width: min(var(--max-width), calc(100% - 82px));
        --max-width: 728px;
        --alt-text-color: rgba(0, 0, 0, 0.6);
        --padding: 32px;
        --max-dialog-height: 0;
    }
    
    .story-dialog::part(base) {
        top: 130px;
        right: 15px;
        bottom: 36px;
        width: var(--width);
        margin: auto;
    }
    
    .story-dialog::part(body) {
        padding-top: 0;
        --body-spacing: 0;
        overflow: hidden;
        border-radius: 8px;
        
    }
    
    .story-dialog::part(header) {
        z-index: 1;
        position: absolute;
        right: -56px;
        --header-spacing: 0;
    }
    
    
    .story-dialog::part(close-button) {
        border-radius: var(--sl-border-radius-large);
        height: 42px;
        width: 42px;
        display: flex;
        justify-content: center;
        background-color: var(--sl-color-neutral-50);
        box-shadow: rgba(0, 0, 0, 0.15) 0px 5px 15px 0px;
    }
    
    .story-dialog::part(panel) {
        max-height: min(var(--max-dialog-height), calc(100% - var(--sl-spacing-2x-large))); /** Take min value from max height constraint or typical screen height */
        min-height: 200px;
        height: 100%;
        border-radius: 8px;
        position: relative;
    }

    .story-scroll {
        height: 100%;
        width: 100%;
        overflow: overlay;
    }

    .story-scroll p{
        line-height:1.8;
    }
    
    .story-container {
        border-radius: 8px;
        height: 100%;
        padding: 0 var(--padding);
        border-radius: 8px;
    }
    
    .story-scroll::-webkit-scrollbar {
        display: none;
    }
    
    img {
        max-width: 100%;
        border-radius: 8px;
    }
    
    p {
        color: var(--sl-color-neutral-1000);
        line-height: 1.8;
    }
    
    .story-header .title {
        font-size: 28px;
        font-weight: var(--sl-font-weight-semibold);
        max-width: max(400px, 60%);
    }
    
    .story-header .award-wrap {
        display: flex;
        align-items: center;
        gap: 8px;
        margin-top: 12px;
        font-size: 12px;
        color: var(--sl-color-neutral-600);
    }
    
    .story-header .award-wrap img {
        height: 20px;
        width: 20px;
        border-radius: 4px;
        vertical-align: bottom;
    }
    
    .header-image {
        width: 100%;
        background-color: var(--sl-color-neutral-200);
    }
    
    /** group app story styles */
    .story-container.GroupProductEditorial {
        .story-header-wrap {
            margin: 0 calc(-1 * var(--padding));
        }
        
    }
    .story-container.GroupProductEditorial .header-image {
        border-radius: 0;
        -webkit-mask-image: linear-gradient(to top, transparent, white 36%, black);
        mask-image: linear-gradient(to top, transparent, white 36%, black);
    }

    .story-collection {
        display: flex;
        flex-direction: column;
        gap: 16px;
    }
    
    /** single app story styles */
    .story-container.SingleProductEditorial {
        margin-top:32px;
    }
    .story-container.SingleProductEditorial::after {
        content: "";
        display: block;
        height: 108px;
        width: 100%;
    }
    
    .story-paragraph {
        font-weight: 400;
    }
    
    .story-paragraph .alt-text {
        color: var(--alt-text-color);
        font-size: 12px;
        margin-bottom: 24px;
    }

    .story-paragraph img {
        margin: 8px 0;    
    }
    
        
    .sticky-header-container {
        position: absolute;
        width: 100%;
    }
    
    .copy-link {
        margin: 32px 0;
        min-width: 180px;
    }

    .copy-link::part(label) {
        display: flex;
        align-items: center;
        gap: 8px;
    }

    .sticky-header {
        --sticky-header-width: calc(min(var(--max-width), 100%) - 24px);
        background-image: var(--theme-noise-image-to-linear-gradient);
        backdrop-filter: blur(100px);
        border: 1px solid var(--sl-color-gray-200);
        border-radius: var(--sl-border-radius-large);
        position: absolute;
        transition: bottom var(--sl-transition-medium) ease-in-out, opacity var(--sl-transition-medium) ease-in-out;
        bottom: 18px;
        width: var(--sticky-header-width);
        opacity: 0;
        padding: var(--sl-spacing-small);
        box-sizing: border-box; /** So that setting the width in JS won't be affected by our padding. */
        box-shadow: var(--sl-shadow-large);
        z-index: var(--sl-z-index-drawer);
        display: flex;
        align-items: center;
        gap: var(--sl-spacing-small);
        justify-content: space-between;
        left: 0;
        right: 0;
        margin: auto;
    }

    .sticky-header.show {
        opacity: 0.95;
        z-index: 1;
    }

    .sticky-header:hover {
        filter: brightness(1.05);
    }

    .sticky-header img {
        border-radius: var(--sl-border-radius-large);
        max-width: 55px;
        height: auto;
    }

    .sticky-header h1 {
        margin-top: 0;
        margin-bottom: 4px;
        font-weight: var(--sl-font-weight-semibold);
        font-size: 18px;
    }

    .sticky-header .title-and-publisher {
        flex-grow: 2;
        flex-basis: 0;
    }
    
    @media(prefers-color-scheme: dark) {
        .story-dialog {
            --alt-text-color: rgba(255, 255, 255, 0.6);
        }
    }
    
    ${tt(Z.xs)} {
        .story-dialog {
            --width: 100%;
            --padding: 12px;
        }
        
        .story-header .title {
            max-width: 100%;
        }
        
        .title-and-publisher {
            display: none;
        }
        
        .story-dialog::part(base) {
            top: 12 0px;
            bottom: 0;
        }
    
        .story-dialog::part(header) {
            right: 16px;
            top: 8px;
            z-index: 100;
        }
    
        .story-dialog::part(panel) {
            max-width: calc(100% - var(--sl-spacing-large));
        }
        
        .story-dialog::part(close-button) {
            height: 24px;
            width: 24px;
